<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<!--监测数据库配置-->
		<section name="checkDatabases" type="System.Configuration.DictionarySectionHandler"/>
		<!--监测进程配置-->
		<section name="checkProcesses" type="System.Configuration.DictionarySectionHandler"/>
	</configSections>

	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
	</startup>

	<!--数据库-->
	<checkDatabases>
		<add key="人员定位" value="mysql|fastgithub" />
		<add key="虹膜考勤" value="oracle|fastgithub" />
	</checkDatabases>

	<!--进程-->
	<checkProcesses>
		<add key="联网上报" value="process1" />
		<add key="报警辅助" value="process2" />
	</checkProcesses>
</configuration>