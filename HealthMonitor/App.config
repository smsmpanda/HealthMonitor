<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<!--监测数据库配置-->
		<section name="checkDatabases" type="System.Configuration.DictionarySectionHandler" />
		<!--监测进程配置-->
		<section name="checkProcesses" type="System.Configuration.DictionarySectionHandler" />
		<section name="oracle.manageddataaccess.client" type="OracleInternal.Common.ODPMSectionHandler, Oracle.ManagedDataAccess, Version=4.122.19.1, Culture=neutral, PublicKeyToken=89b483f429c47342" />
	</configSections>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
	</startup>
	<!--数据库-->
	<checkDatabases>
		<add key="人员定位" value="oracle|Data Source=192.168.132.216/MKDB3D;Persist Security Info=True;User ID=mkgk;Password=mkgk;Connection Timeout=3" />
	</checkDatabases>
	<!--进程-->
	<checkProcesses>
		<add key="联网上报" value="WechatBrowser" />
		<add key="报警辅助" value="HealthMonitor" />
		<add key="向日葵" value="SunloginClient" />
	</checkProcesses>
	<connectionStrings>
		<!--报警信息存储数据库-->
		<add name="RYDWDb" connectionString="Data Source=192.168.132.216/MKDB3D;Persist Security Info=True;User ID=mkgk;Password=mkgk;"></add>
	</connectionStrings>
	<system.data>
		<DbProviderFactories>
			<remove invariant="Oracle.ManagedDataAccess.Client" />
			<add name="ODP.NET, Managed Driver" invariant="Oracle.ManagedDataAccess.Client" description="Oracle Data Provider for .NET, Managed Driver" type="Oracle.ManagedDataAccess.Client.OracleClientFactory, Oracle.ManagedDataAccess, Version=4.122.19.1, Culture=neutral, PublicKeyToken=89b483f429c47342" />
		</DbProviderFactories>
	</system.data>
	<runtime>
		<assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
			<dependentAssembly>
				<publisherPolicy apply="no" />
				<assemblyIdentity name="Oracle.ManagedDataAccess" publicKeyToken="89b483f429c47342" culture="neutral" />
				<bindingRedirect oldVersion="4.121.0.0 - 4.65535.65535.65535" newVersion="4.122.19.1" />
			</dependentAssembly>
		</assemblyBinding>
	</runtime>
</configuration>